<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp8.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp8"
    Title="MauiApp8">
    <TabBar>
        <Tab Title="MAUI 14.2">
            <ShellContent
                Title="MAUI 14.2"
                ContentTemplate="{DataTemplate local:MainPage}"
                Route="MainPage" />
        </Tab>
        <Tab Title="MAUI 14.3">
            <ShellContent
                Title="MAUI 14.3"
                ContentTemplate="{DataTemplate local:NewPage1}"
                Route="NewPage1" />
        </Tab>
        <Tab Title="MAUI 14.4">
            <ShellContent
                Title="MAUI 14.4"
                ContentTemplate="{DataTemplate local:NewPage2}"
                Route="NewPage2" /> 
        </Tab>
    </TabBar>
</Shell>
